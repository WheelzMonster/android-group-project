<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="fr.epsi.ted_app">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".TED_App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_controller"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_controller"
        android:supportsRtl="true"
        android:theme="@style/Theme.TED_APP">
        <activity
            android:name=".CategorieOnlineActivity"
            android:exported="false" />
        <activity
            android:name=".DetailActivity"
            android:exported="false" />
        <activity
            android:name=".RayonActivity"
            android:exported="false" />
        <activity
            android:name=".EtudiantActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="false" />
        <activity
            android:name=".SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>